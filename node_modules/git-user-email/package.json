{
  "name": "git-user-email",
  "description": "Get the email address of the current user from git config.",
  "version": "0.2.2",
  "homepage": "https://github.com/jonschlinkert/git-user-email",
  "author": {
    "name": "Jon Schlinkert",
    "url": "http://github.com/https://github.com/jonschlinkert/",
    "username": "jonschlinkert",
    "twitter": "jonschlinkert"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonschlinkert/git-user-email.git"
  },
  "bugs": {
    "url": "https://github.com/jonschlinkert/git-user-email/issues"
  },
  "license": {
    "type": "MIT",
